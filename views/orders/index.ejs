<% if (orders.length == 0){%>
There is no items in the order?!
<%} else {%>
<div class="container">
    <% orders.forEach ((order, count) => {%>
    <table>
        <tr>Order <%= count +1 %></tr>
        <tr>
            <td>Items</td>
            <td>Quantity</td>
            <td>Price</td>
        </tr>
        <% order.items.forEach(item => { %>
        <tr>
            <td><%= item.item.name %></td>
            <td><%= item.quantity %></td>
            <td><%= item.item.price %></td>
        </tr>
        <% }) %>
    </table>
    <% }) %>
</div>
<% } %>